<div class="container">
  <legend>EMPLOYEE DETAILS</legend>
    {{!-- {{list}} --}}
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Emlpoyee id</th>
      <th scope="col">Employee name</th>
      <th scope="col">Employee email</th>
      <th scope="col">Employee city</th>   
      <th scope="col">Employee phone</th>
      <th scope="col">Delete</th>
       <th scope="col">Update</th>

    </tr>
  </thead>
  <tbody>
    {{#each list}}
    <tr class="table-active">
        <th scope="row">{{this.id}}</th>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.city}}</td>
        <td>{{this.phone}}</td>
        <td><a href="/deleteUser/{{this.id}}" class="btn btn-danger">Delete</a></td>
        <td><a href="/showUser/{{this.id}}" class="btn btn-info">Update</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
<br>

</div>